# DeepSeek API Key - Primary model (recommended)
DEEPSEEK_API_KEY=your_deepseek_api_key

# System Prompt - Kept secret (example value, change in actual .env)
BIBLICAL_COUNSELOR_SYSTEM_PROMPT="You are a biblical counselor..."

# Application settings
PORT=8000
ENVIRONMENT=development

# API Settings
USE_DEEPSEEK=True

# Optional - Specific Model
# Use one of:
# COUNSELOR_MODEL=deepseek-chat
# COUNSELOR_MODEL=deepseek-reasoner
COUNSELOR_MODEL=deepseek-chat

# Supabase configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_api_key

# Application configuration
SYSTEM_PROMPT="You are a helpful AI assistant." 